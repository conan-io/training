package conan.ci.step

@Library('demo_jenkins_library') _

pipeline {
    agent any
    stages {
        stage('test') {
            environment{
                DEMO_LOG_LEVEL = "DEBUG"
                DEMO_GIT_CREDS = "usr:psw"
                DEMO_GIT_CREDS_USR = "usr"
                DEMO_GIT_CREDS_PSW = "psw"
                DEMO_CONAN_CONFIG_URL = "http://gitbucket/git/root/settings.git"
            }
            steps {
                conanConfigInstall()
            }
        }
    }
}